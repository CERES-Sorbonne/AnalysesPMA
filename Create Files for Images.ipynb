    "def map_values(dates, res):\n",
    "    values = []\n",
    "    for d in dates:\n",
    "        found = False\n",
    "        for el in res:\n",
    "            if el[0] == d:\n",
    "                values.append(el[1])\n",
    "                found = True\n",
    "        if not found:\n",
    "            values.append(0)\n",
    "    return values"
   ]
