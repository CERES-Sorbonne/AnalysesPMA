    "def get_weeks_between(start, end):\n",
    "    delta = end - start\n",
    "    curr_date = start\n",
    "    all_dates = [curr_date]\n",
    "    while curr_date < end:\n",
    "        curr_date += timedelta(7)\n",
    "        all_dates.append(curr_date)\n",
    "    return all_dates"
   ]
  },
