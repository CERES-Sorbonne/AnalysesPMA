    "from matplotlib.ticker import AutoMinorLocator, FixedLocator, FormatStrFormatter, NullFormatter\n",
    "from matplotlib.dates import date2num, DateFormatter\n",
    "import statistics\n",
    "import matplotlib\n",
    "from PIL import Image\n",
    "\n",
    "matplotlib.rcParams.update({'font.size': 12})\n",
    "\n",
    "def make_plot(dates, values, hash_, folder):\n",
    "    ax = plt.subplot(111)\n",
    "    ax.plot(dates, values) #, width=2, align=\"center\")\n",
    "    # calculer un seuil correspondant au dernier octile\n",
    "    v = [x for x in values if x > 0]\n",
    "    treshold = statistics.quantiles(v, n=8)[-1]\n",
    "\n",
    "    # calculer quelles dates on dcide d'afficher (valeus dpassant le seuil)\n",
    "    picks = [d for d, y in zip(dates, values) if y > treshold and d not in dates_pma]\n",
    "    # dfinir quels ticks sont majeurs et quels sont mineurs\n",
    "    ax.xaxis.set_minor_locator(FixedLocator([*date2num(picks)]))\n",
    "    ax.xaxis.set_major_locator(FixedLocator([*date2num(dates_pma)]))\n",
    "    # changer leur apparence\n",
    "    ax.xaxis.set_tick_params(which='major', length=0, color='red')\n",
    "    ax.xaxis.grid(True, which='major', color=\"red\", linestyle='dashed')\n",
    "    ax.tick_params(which='minor', length=10, color='black')\n",
    "    \n",
    "    # formater les dates\n",
    "    hfmt = matplotlib.dates.DateFormatter('%Y-%m-%d')\n",
    "    ax.xaxis.set_minor_formatter(hfmt)\n",
    "    ax.xaxis.set_major_formatter(NullFormatter())\n",
    "    # afficher les valeurs au dessus des points\n",
    "    for date, value in zip(dates, values):\n",
    "        if value > statistics.quantiles(v, n=8)[1]:\n",
    "            ax.text(x=date, y=value, s=str(value or ''), fontdict=dict(fontsize=10))\n",
    "    \n",
    "    plt.title(f\"Number of images mentions of {hash_} per day\")\n",
    "    plt.gcf().set_size_inches(18.5, 14)\n",
    "    \n",
    "    # changer la rotation des ticks majeurs\n",
    "    plt.xticks(rotation=90)\n",
    "    # et celle des ticks mineurs\n",
    "    plt.setp(ax.xaxis.get_minorticklabels(), rotation=90)\n",
    "\n",
    "    # [t.set_color('red') for t in ax.xaxis.get_ticklabels()]\n",
    "    \n",
    "    for xi, t, l in zip(ax.xaxis.get_majorticklocs(), dates_pma, labels_pma):\n",
    "        ax.text(xi,0.4, t.strftime(\"%Y-%m-%d\") + \"-\"*10 + l, clip_on=True, ha=\"center\",\n",
    "                transform=ax.get_xaxis_transform(), rotation=90, color='red',\n",
    "                bbox={'facecolor':'w', 'pad':1, \"edgecolor\":\"None\"})\n",
    "    \n",
    "    fig_path = os.path.join(folder, f'histo-{hash_}.png')\n",
    "#     plt.savefig(fig_path, transparent=False, facecolor='white')\n",
    "#     Image.open(fig_path).convert('RGB').save(fig_path.split('.')[0] + '.jpg', 'JPEG', dpi=(400, 400))\n",
    "    plt.show()\n",
    "    plt.clf()"
   ]
