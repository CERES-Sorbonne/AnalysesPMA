    "    uid = tweet['author_id']\n",
    "    username = [u['username'] for u in users if u['id'] == uid][0]\n",
    "    for index, key in enumerate(tweet.get('attachments', {}).get('media_keys', [])):\n",
    "        for medium in media:\n",
    "            if medium['media_key'] == key:\n",
    "                sha1 = download_media(medium['media_key'], medium['url'])\n",
    "                print(sha1)\n",
    "        res.loc[res.shape[0]] = [username] + [sha1] + [tweet['public_metrics']['retweet_count'] + 1] + [tweet['text']]"
   ]
